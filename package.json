{
  "name": "@example/with-tailwindcss",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint --fix",
    "tailwind:build": "postcss tailwind.css -o tailwind.min.css",
    "tailwind:watch": "postcss tailwind.css -o tailwind.min.css --watch",
    "tailwind:build:npx": "npx tailwindcss -i ./src/styles/global.css -o ./dist/output.css --watch",
    "format:code": "prettier -w ./src --cache --plugin-search-dir=.",
    "format:code:dry": "prettier  ./src --cache --plugin-search-dir=.",
    "format:imports": "organize-imports-cli ./packages/*/tsconfig.json ./packages/*/*/tsconfig.json",
    "astro": "astro"
  },
